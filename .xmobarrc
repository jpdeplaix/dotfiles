       , bgColor  = "#000000"
       , fgColor  = "#BFBFBF"
       , position = Top
       , lowerOnStart = True
       , commands = [ Run Date "%a %b %_d %Y * %H:%M:%S" "theDate" 10
                    , Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
                    , Run Memory ["-t","Mem: <usedratio>%"] 10
                    , Run StdinReader
                    , Run ThermalZone 9 ["-t","<temp>Â°C"] 100
                    , Run Wireless "wlan0" [] 100
                    , Run Battery [] 500
                    ]
       , sepChar  = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{ %battery% <fc=#FF0000>*</fc> %wlan0wi% <fc=#FF0000>*</fc> %thermal9% <fc=#FF0000>*</fc> %cpu% <fc=#FF0000>*</fc> %memory% <fc=#FF0000>*</fc> %theDate%"
